<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nju.iip.dto.WeiboData" table="WeiboData">
		<meta attribute="class-description">
			This class contains the nju.iip.dto.WeiboData detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="baseUrl" column="baseUrl" type="string" />
		<property name="author" column="author" type="string" />
		<property name="text" column="text" type="string" />
		<property name="loves" column="loves" type="int" />
		<property name="repost" column="repost" type="int" />
		<property name="comment" column="comment" type="int" />
		<property name="device" column="device" type="string" />
		<property name="time" column="time" type="string" />
		<property name="inputTime" column="inputTime" type="string" />
	</class>
</hibernate-mapping>