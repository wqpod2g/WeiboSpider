<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="nju.iip.dto.TargetWeiboAccount" table="TargetWeiboAccount">
		<meta attribute="class-description">
			This class contains the nju.iip.dto.TargetWeiboAccount detail.
		</meta>
		<id name="id" type="int" column="id">
			<generator class="native" />
		</id>
		<property name="accountName" column="accountName" type="string" />
		<property name="accountUrl" column="accountUrl" type="string" />
		<property name="history" column="history" type="int" />
	</class>
</hibernate-mapping>